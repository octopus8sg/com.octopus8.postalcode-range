<?xml version="1.0"?>
<extension key="postalcode_range" type="module">
  <file>postalcode_range</file>
  <name>postalcode_range</name>
  <description>Postal Code</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>MayPhyu</author>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-06-24</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.72</ver>
  </compatibility>
  <comments>This extension has a form to enter the postal codes and stores the postal codes in the database table. If the postal code of each contact is the same as one of the postal codes within the database table, each contact will be assigned a tag automatically.</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/PostalcodeRange</namespace>
    <format>23.02.1</format>
    <angularModule>crmPostalcodeRange</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_PostalcodeRange_Upgrader</upgrader>
</extension>
